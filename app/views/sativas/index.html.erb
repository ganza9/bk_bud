<p id="notice"><%= notice %></p>

<h1>Sativas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sativas.each do |sativa| %>
      <tr>
        <td><%= link_to 'Show', sativa %></td>
        <td><%= link_to 'Edit', edit_sativa_path(sativa) %></td>
        <td><%= link_to 'Destroy', sativa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sativa', new_sativa_path %>
