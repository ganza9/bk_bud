<p id="notice"><%= notice %></p>

<h1>Indicas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @indicas.each do |indica| %>
      <tr>
        <td><%= link_to 'Show', indica %></td>
        <td><%= link_to 'Edit', edit_indica_path(indica) %></td>
        <td><%= link_to 'Destroy', indica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indica', new_indica_path %>
